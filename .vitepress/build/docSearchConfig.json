{
  "index_name": "cz-git",
  "start_urls": ["https://cz-git.qbb.sh", "https://cz-git.qbb.sh/zh/"],
  "stop_urls": [],
  "selectors": {
    "lvl0": {
      "selector": ".VPLink.active",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": ".VPDoc h1",
    "lvl2": ".VPDoc h2",
    "lvl3": ".VPDoc h3",
    "lvl4": ".VPDoc h4",
    "lvl5": ".VPDoc h5",
    "text": ".VPDoc p, .VPDoc li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  }
}
